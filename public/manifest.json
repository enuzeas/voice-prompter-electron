{
    "manifest_version": 3,
    "name": "Focus Prompter - Presentation Window",
    "version": "0.0.6",
    "description": "Voice-activated teleprompter in your side panel.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "512": "icons/icon512.png"
    },
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "activeTab"
    ]
}